#!/bin/sh
set -ex
cp $1/json_codec_typescript_test.ts .
cp $2 .
/home/xju/node_modules/typescript/bin/tsc --traceResolution --sourceMap --moduleResolution node --module nodenext --lib es2015 --strict "json_codec_typescript_test.ts" --outDir . >&2
/home/xju/node_modules/typescript/bin/tsc --traceResolution --sourceMap --moduleResolution node --module nodenext --lib es2015 --strict $(basename "$2") --outDir . >&2

%json_codec_typescript_test.sh==!<<
#!/bin/sh
set -e
cp $1 test-json-codec-typescript.js
cp $2 json_codec_typescript_test.js
out=$(NODE_PATH=$(dirname $1) NODE_V8_COVERAGE=. node --test --experimental-test-coverage test-json-codec-typescript.js) || (
  echo "$out" >&2
  exit 1
)
echo "$out" | grep json_codec_typescript_test.js | (
  read a b c d e f g h i lines_not_covered
  python3 -c "print('\n'.join([f'error:$2:{_}: not covered' for _ in '$lines_not_covered'.split(',')]))"
  test -z "$lines_not_covered"
)

